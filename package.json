  "devDependencies": {
    "acorn": "^2.4.0",
    "escodegen": "^1.7.0",
    "esfuzz": "~0.3.1",
    "estraverse": "^4.1.0"
  },
  "browserify": {
    "transform": [
      "uglify-to-browserify"
    ]
  },
  "scripts": {
    "shrinkwrap": "rm ./npm-shrinkwrap.json; rm -rf ./node_modules; npm i && npm shrinkwrap && npm outdated",
    "test": "node test/run-tests.js"
